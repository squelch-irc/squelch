{
  "name": "squelch",
  "productName": "Squelch",
  "version": "0.0.0",
  "description": "An IRC client",
  "private": "true",
  "main": "main.js",
  "scripts": {
    "start": "npm run start-dev",
    "start-dev": "cross-env NODE_ENV=development DEBUG=squelch-client npm run start-prod",
    "start-prod": "electron .",
    "clean": "rimraf release/",
    "lint": "standard",
    "prebuild": "npm run clean && npm run lint",
    "build": "electron-packager . --all --out release --prune --ignore='^/test$'",
    "pretest": "npm run lint",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/squelch-irc/squelch.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/squelch-irc/squelch/issues"
  },
  "homepage": "https://github.com/squelch-irc/squelch#readme",
  "devDependencies": {
    "ava": "^0.15.2",
    "cross-env": "^1.0.8",
    "devtron": "^1.2.0",
    "electron-packager": "^7.0.3",
    "electron-prebuilt": "^1.2.2",
    "electron-rebuild": "^1.0.2",
    "rimraf": "^2.4.1",
    "standard": "^8.6.0"
  },
  "dependencies": {
    "barracks-promisify-plugin": "0.0.1",
    "bluebird": "^3.3.4",
    "catw": "^1.0.1",
    "checkit": "^0.7.0",
    "choo": "^4.0.1",
    "classnames": "^2.2.5",
    "concat-stream": "^1.5.1",
    "electron-config": "^0.2.1",
    "electron-debug": "^1.0.0",
    "electron-localshortcut": "^1.0.0",
    "event-kit": "^2.0.0",
    "freezer-js": "^0.11.1",
    "fs-extra": "^0.30.0",
    "get-key-range": "^1.0.0",
    "irc-networks": "^1.0.1",
    "lodash": "^4.17.2",
    "lodash.mapvalues": "^4.4.0",
    "lodash.omit": "^4.3.0",
    "moment": "^2.10.3",
    "object-path-immutable": "^0.5.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-router": "^2.0.0-rc4",
    "react-treeview": "^0.4.3",
    "require-dir": "^0.3.0",
    "squelch-base-theme": "^0.0.11",
    "squelch-client": "^0.19.0",
    "squelch-nick-hash": "^1.0.1",
    "string-args": "^1.0.2",
    "tachyons": "^4.5.6",
    "tachyons-svg": "^1.0.0",
    "username": "^2.2.2"
  }
}
